[ req ]
default_bits       = 2048
distinguished_name = req_distinguished_name
req_extensions     = req_ext
prompt             = no
[ req_distinguished_name ]
countryName                = GB
organizationName           = Layout Server
organizationalUnitName     = local
commonName                 = vuu.server.local
[ req_ext ]
subjectAltName = @alt_names
[alt_names]
DNS.1   = vuu.server.local
DNS.2   = 127.0.0.1
DNS.3   = localhost
DNS.4   = IP:vuu.server.local
DNS.5   = IP:127.0.0.1
DNS.6   = IP:localhost